  "scripts": {
    "ci": "bun install --frozen-lockfile --production",
    "update-deps": "rm node_modules bun.lock -rf && bun i",
    "build": "prisma generate --no-engine && next build",
    "start": "next start",
    "dev": "next dev --turbopack -p 3000",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev --turbopack -p 3000",
    "devsafe": "rm -rf .next && cross-env next dev --turbopack -p 3000",
    "lint": "ultracite lint",
    "format": "ultracite format",
    "lint:fix": "biome lint --write ./src",
    "format:fix": "biome format --write ./src",
    "fix:ci": "biome ci --formatter-enabled=true --linter-enabled=true --organize-imports-enabled=true --assists-enabled=true",
    "db:push": "prisma db push",
    "db:generate": "prisma generate --no-engine",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:tunnel": "bunx @prisma/ppg-tunnel"
  },
